class Solution {
    public int missingNumber(int[] nums) {
        Arrays.sort(nums);
        for(int i=0;i<nums.length;i++){
            if(i==nums[i]){
                continue;
            }
            else{
                int num = i;
                return num;
            }
        }
        int num = nums.length;
        return num;
        
    }
}
