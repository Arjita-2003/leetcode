class Solution {
    public int[] getConcatenation(int[] nums) {
        int n = nums.length;
        int[] arr = new int[2*n];
        int j = 0;
        int k=0;
        while(k<2){
            for(int i=0;i<n;i++){
                arr[j++] = nums[i];
            }
            k++;
        }
        return arr;
    }
}
